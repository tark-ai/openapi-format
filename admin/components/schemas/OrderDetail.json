          "type": "string"
        }
      }
    },
    "feedback": {
      "type": [
        "string",
        "null"
      ]
    },
    "allowed_actions": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "applied_coupons": {
      "type": "array",
      "items": {
        "$ref": "./AppliedCoupon.json"
      }
    },
    "applied_promotions": {
      "type": "array",
      "items": {
        "$ref": "./AppliedPromotion.json"
      }
    },
    "payments": {
      "type": "array",
      "items": {
        "$ref": "./OrderPayment.json"
      }
    },
    "shipments": {
      "type": "array",
      "items": {
        "$ref": "./OrderShipment.json"
      }
