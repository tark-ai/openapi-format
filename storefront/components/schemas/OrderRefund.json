    },
    "payment_method": {
      "type": "string"
    },
    "payment_reference_number": {
      "type": "string"
    },
    "bank_account_number": {
      "type": "string"
    },
    "bank_name": {
      "type": "string"
    },
    "refund_remarks": {
      "type": "string"
    }
  },
  "externalDocs": {
