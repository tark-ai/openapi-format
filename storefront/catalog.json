          },
          {
            "name": "category_id",
            "in": "query",
            "description": "filter products by categories ids",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "explode": true,
            "style": "form"
          },
          {
            "name": "category_slug",
